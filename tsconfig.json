{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@fawry_pay/rn-fawry-pay-sdk": [
        "./node_modules/@fawry_pay/rn-fawry-pay-sdk/src/index.tsx"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "node_modules/@fawry_pay/rn-fawry-pay-sdk/src/**/*"
  ]
}
